            $(document).on("click", ".finish-resume", function (e) {

                e.preventDefault();

                var educations = new Array();
                $(".edu-card").each((index, item) => {
                    educations.push({
                        SchoolName: $(item).find('input[name="resume-edu-school"]').val(),
                        Qualification: $(item).find('input[name="resume-edu-quality"]').val(),
                        StartSchool: $(item).find('select[name="resume-edu-since"]').val(),
                        EndSchool: $(item).find('select[name="resume-edu-till"]').val(),
                    })
                })


                var works = new Array();
                $(".work-card").each((index, item) => {
                    works.push({
                        CompanyName: $(item).find('input[name="resume-exp-company"]').val(),
                        Position: $(item).find('input[name="resume-exp-position"]').val(),
                        StartWork: $(item).find('select[name="resume-exp-since"]').val(),
                        EndWork: $(item).find('select[name="resume-exp-till"]').val(),
                    })
                })


                var resumedata = new FormData();
                resumedata.append("jobProfession", $("input[name=resume-input-profession]").val());
                resumedata.append("personalSkill", $("input[name=resume-input-personal]").val());
                resumedata.append("ExperienceYear", $("select[name=resume-input-exp] option:selected").text());

                for (let i = 0; i < works.length; i++) {
                    resumedata.append("works[" + i + "].CompanyName", works[i].CompanyName);
                    resumedata.append("works[" + i + "].Position", works[i].Position);
                    resumedata.append("works[" + i + "].StartWork", works[i].StartWork);
                    resumedata.append("works[" + i + "].EndWork", works[i].EndWork);
                }


                for (let i = 0; i < educations.length; i++) {
                    resumedata.append("educations[" + i + "].SchoolName", educations[i].SchoolName);
                    resumedata.append("educations[" + i + "].Qualification", educations[i].Qualification);
                    resumedata.append("educations[" + i + "].StartSchool", educations[i].StartSchool);
                    resumedata.append("educations[" + i + "].EndSchool", educations[i].EndSchool);
                }

                $.ajax({
                    url: "/Resume/Create",
                    type: "post",
                    dataType: "json",
                    data: resumedata,
                    processData: false,
                    contentType: false,
                    beforeSend: function () {
                    },
                    success: function (response) {
                        if (response.statusCode === 500) {
                        } else {
                            if (response.isRedirect) {
                                window.location.href = response.redirectUrl;
                            }
                        }

                    },
                    error: function (error) {
                    },
                    complete: function () {
                    }
                })
            })

